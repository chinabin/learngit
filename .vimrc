set nocompatible
syntax enable
filetype plugin indent on
filetype on
"for colorscheme solarized
syntax enable
set background=dark
colorscheme desert
highlight StatusLine guifg=SlateBlue guibg=Yellow 
set showmatch
set nobackup
setlocal noswapfile
set wildmenu
set ruler
set backspace=2
set hlsearch
set scrolloff=5
set novisualbell
set autoindent
set smartindent
set cindent
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set nolist

"for hightlight current line
set cursorline
hi CursorLine cterm=NONE ctermbg=darkred ctermfg=white
hi CursorColumn cterm=NONE ctermbg=darkred ctermfg=white
